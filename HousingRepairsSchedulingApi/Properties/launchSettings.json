      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "AUTHENTICATION_IDENTIFIER": "Foo",
        "JWT_SECRET" : "<REPLACE WITH A SECRET>",
        "DrsOptions__ApiAddress" : "https://<REPLACE WITH DRS API ADDRESS>",
